 {\n    this.init();\n  }\n};","map":{"version":3,"names":["addScoreData","getScoreProjectList","getUserList","getOneScoreProjectList","name","components","data","submitLoading","form","projectId","projectName","studentId","studentName","value","reason","time","workId","workName","formValidate","projectList","studentList","tempContext","methods","init","getScoreProjectListFx","getUserListFx","changeProject","e","that","id","then","res","success","result","content","type","handleReset","$refs","resetFields","handleSubmit","validate","valid","$Message","submited","close","$emit","mounted"],"sources":["src/views/score/scoreData/add.vue"],"sourcesContent":["<template>\r\n<div>\r\n    <Card>\r\n        <div slot=\"title\">\r\n            <div class=\"edit-head\">\r\n                <a @click=\"close\" class=\"back-title\">\r\n                    <Icon type=\"ios-arrow-back\" />返回\r\n                </a>\r\n                <div class=\"head-name\">新增加扣分数据</div>\r\n                <span></span>\r\n                <a @click=\"close\" class=\"window-close\">\r\n                    <Icon type=\"ios-close\" size=\"31\" class=\"ivu-icon-ios-close\" />\r\n                </a>\r\n            </div>\r\n        </div>\r\n        <Form ref=\"form\" :model=\"form\" :label-width=\"100\" :rules=\"formValidate\" label-position=\"left\">\r\n            <FormItem label=\"评分项目\" prop=\"projectId\">\r\n                <Select v-model=\"form.projectId\" clearable @on-change=\"changeProject\" placeholder=\"请选择评分项目...\" style=\"width:570px\">\r\n                    <Option v-for=\"(item,index) in projectList\" :key=\"index\" :value=\"item.id\">{{ item.title }}</Option>\r\n                </Select>\r\n            </FormItem>\r\n            <FormItem label=\"评分细则\">\r\n                <Input v-model=\"tempContext\" readonly type=\"textarea\" :autosize=\"true\" placeholder=\"请先选择评分项目...\" style=\"width:570px\" />\r\n            </FormItem>\r\n            <FormItem label=\"学生\" prop=\"studentId\">